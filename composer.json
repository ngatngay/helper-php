{
    "name": "ngatngay/helper",
    "description": "Simple Helper for PHP",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "ngatngay",
            "email": "ngatngay@ngatngay.net"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "NgatNgay\\Helper\\": "src/"
        },
        "files": [
            "src/function.php"
        ]
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "symfony/var-dumper": "7.3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
